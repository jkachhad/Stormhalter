<data>
  <!--
	Categories define where items may be stored.
	Category	Definition
	1		Large Weapon/Back Belt
	2		Small Weapon/Belt
	3		Misc Sack :  ex: A Gem
	4		Ring/Sack
	5		Helmet/Head/Belt/Sack
	6		Amulet/Neck/Sack/Belt
	7		Bracelet/Sack
	8		Gauntlets/Belt/Sack
	9		Armor/Back Belt
	10		Robe/Back Belt
	11		Boots/Belt/Sack
	12		Consumable/Sack
	13		Coin
	14		Dagger/Sack/Belt/throw from belt
	15		Halberd/Back Belt/etc
	16		Corpse
  -->
  <!-- Example 
  <item id="900">
    <texture>Sprites/Items/Items-1</texture>
    <source>(855,855,45,45)</source>
  </item>
  -->
  <!-- Corpse all start at 5000 + bodyId, but can be configured in Body.xml -->
  <!-- Example
  <item id="5001" category="16">
    <texture>Bitmaps\002</texture>
    <source>(10,10,45,45)</source>
  </item>
  -->
